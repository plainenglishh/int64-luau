local bit64 = require("../../init")

local lshift_n = bit64.from_pair(0x00000000, 0x00000001)
for i = 0, 63 do
	local new = bit64.lshift(lshift_n, i)
	local expected_bin = string.rep("0", 63 - i) .. "1" .. string.rep("0", i)
	assert(bit64.to_bin_string(new) == expected_bin, `{bit64.to_hex_string(lshift_n)} << {i} was wrong`)
end
assert(bit64.lshift(lshift_n, 64) == vector.zero, `{bit64.to_hex_string(lshift_n)} << 64 was wrong`)

